{
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:next": "tsc",
    "postbuild": "rm -rf .parcel-cache && parcel build ./ --dist-dir ../public --public-url ./ && rm -rf .parcel-cache dist",
